{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
        {"include": "../appconfigs/*.yaml", "outDir": "./dist/appconfigs"},
        {"include": "../src/data/*", "outDir": "./dist/src/"}
      ]
  },
  "plugins": [
    {
      "name": "@nestjs/swagger",
      "options": {
        "dtoFileNameSuffix" : [".ts"]
      }
    }
  ]
}
