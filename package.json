{
  "name": "noba-server",
  "private": true,
  "version": "1.0.0",
  "description": "Noba NestJS based Server",
  "scripts": {
    "aatest": "npx ts-node ./test/testRun.ts",
    "prebuild": "rimraf dist",
    "prisma-generate": "yarn prisma generate",
    "prisma-clean-init": "yarn prisma db push",
    "prisma-update": "yarn prisma generate && yarn prisma migrate dev --name=init",
    "prisma-migrate-deploy": "yarn prisma generate && yarn prisma migrate deploy",
    "prisma-seed": "yarn prisma db seed",
    "build": "nest build -c nest-cli.json --webpack --webpackPath webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "yarn start:dev",
    "start": "cross-env NODE_ENV=development nest start --webpack --webpackPath webpack.config.js",
    "start:e2e": "cross-env NODE_ENV=e2e_test nest start --webpack --webpackPath webpack.config.js",
    "start:dev": "cross-env NODE_ENV=development nest start --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "nest start --debug  --webpack --webpackPath webpack.config.js --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:prisma-migrate": "yarn prisma-migrate-deploy && yarn test:unit",
    "test": "yarn test:unit && yarn test:integration",
    "test-repos": "jest --testPathPattern='repo' --verbose --coverage --coverageDirectory='coverage-repos' --silent=False",
    "test-controllers": "jest --testPathPattern='controller' --verbose --coverage --coverageDirectory='coverage-controllers' --silent=False",
    "test-others": "jest --testPathIgnorePatterns='repo' --testPathIgnorePatterns='controller' --verbose --coverage --coverageDirectory='coverage-others' --silent=False",
    "test:unit": "jest --runInBand --verbose --silent=False",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:cov": "yarn test:unit --coverage",
    "test:integration": "yarn prisma db seed && jest --verbose --silent --runInBand --config ./test/integration/jest-integration.json",
    "gen-test-api-client": "npx openapi-typescript-codegen --useOptions --input ./swagger-internal.json --output ./test/api_client/ --useUnionTypes --exportCore false -c axios && npx prettier --write ."
  },
  "dependencies": {
    "@aws-crypto/client-node": "^3.1.1",
    "@aws-sdk/client-s3": "^3.272.0",
    "@aws-sdk/client-secrets-manager": "^3.272.0",
    "@circle-fin/circle-sdk": "^1.2.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/units": "^5.7.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.12.0",
    "@sendgrid/mail": "^7.6.2",
    "@temporalio/client": "^1.5.2",
    "@types/crypto-js": "^4.1.1",
    "@types/facebook-nodejs-business-sdk": "^15.0.2",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/sharp": "^0.31.0",
    "axios": "^0.21.4",
    "bad-words-es": "^1.0.0",
    "cache-manager": "^3.6.3",
    "checkout-sdk-node": "^2.0.1",
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "country-list-with-dial-code-and-flag": "^3.0.2",
    "credit-card-type": "^9.1.0",
    "crypto-js": "^4.1.1",
    "csv": "^6.2.0",
    "csv-stringify": "^6.2.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.29.2",
    "dayjs": "^1.11.7",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.1",
    "facebook-nodejs-business-sdk": "^16.0.2",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "google-libphonenumber": "^3.2.32",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "i18next": "^22.4.15",
    "i18next-fs-backend": "^2.1.1",
    "joi": "^17.6.0",
    "joi-to-swagger": "^5.2.0",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multicoin-address-validator": "^0.5.10",
    "nanoid": "^3.1.23",
    "nest-winston": "^1.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "plaid": "^11.0.0",
    "prisma": "^4.12.0",
    "puppeteer": "^19.7.3",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rxjs": "^7",
    "sharp": "^0.31.1",
    "swagger-ui-express": "^4.5.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "tunnel": "^0.0.6",
    "twilio": "^3.84.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/testing": "^9.4.0",
    "@types/credit-card-type": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "~29.2.0",
    "@types/joi": "^17.2.3",
    "@types/js-yaml": "^4.0.3",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multicoin-address-validator": "^0.5.0",
    "@types/node": "^14.17.5",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "circular-dependency-plugin": "^5.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "~29.2.0",
    "jest-cli": "^29.3.1",
    "jest-mock-axios": "^4.6.1",
    "prettier": "^2.6.2",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.9.4",
    "webpack": "^5.76.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^uuid$": "uuid"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.module.ts",
      "!**/transactions/*.ts"
    ],
    "coverageProvider": "v8",
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "../test/matchers/base.exception.errorcode.ts",
      "../test/matchers/service.exception.errorcode.ts",
      "../test/matchers/repo.exception.errorcode.ts"
    ]
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed/seed.ts"
  }
}
