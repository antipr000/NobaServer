{
  "name": "noba-server",
  "private": true,
  "version": "1.0.0",
  "description": "Noba NestJS based Server",
  "scripts": {
    "aatest": "npx ts-node ./test/testRun.ts",
    "prebuild": "rimraf dist",
    "build": "nest build  -c nest-cli.json --webpack --webpackPath webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start --webpack --webpackPath webpack.config.js",
    "start:dev": "cross-env NODE_ENV=development nest start --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "nest start --debug  --webpack --webpackPath webpack.config.js --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest && yarn test:e2e",
    "createtables:dev": "npx ts-node src\\infra\\dynamodb\\scripts\\createddbtables.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ddb-admin": "cross-env DYNAMO_ENDPOINT=http://localhost:4566 dynamodb-admin",
    "gen-test-api-client": "bash ./test/api_client/client.generator.sh"
  },
  "dependencies": {
    "@aws-crypto/client-node": "^3.1.1",
    "@aws/dynamodb-data-mapper": "0.7.3",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/throttler": "^2.0.1",
    "@sendgrid/mail": "^7.6.2",
    "@terra-money/terra.js": "^3.0.9",
    "@types/cache-manager": "^3.4.2",
    "@types/node-fetch": "^2.6.2",
    "aws-sdk": "^2.948.0",
    "axios": "^0.21.4",
    "cache-manager": "^3.6.3",
    "checkout-sdk-node": "^2.0.1",
    "coingecko-api": "^1.0.10",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "csv": "^6.1.3",
    "csv-stringify": "^6.1.0",
    "date-fns": "^2.28.0",
    "ethereumjs-tx": "^2.1.2",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "helmet": "^4.6.0",
    "joi": "^17.6.0",
    "joi-to-swagger": "^5.2.0",
    "joigoose": "^8.0.2",
    "js-base64": "^3.7.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mongodb-memory-server": "^8.6.1",
    "mongoose": "^6.2.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.1.23",
    "nest-router": "^1.0.9",
    "nest-winston": "^1.5.0",
    "openapi-typescript-codegen": "^0.23.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "querystring": "^0.2.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "shallow-equal-object": "^1.1.1",
    "stripe": "^8.210.0",
    "swagger-ui-express": "^4.1.6",
    "ts-mockito": "^2.6.1",
    "twilio": "^3.77.0",
    "web3": "^1.7.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/aws-sdk": "^2.7.0",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/joi": "^17.2.3",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash": "^4.14.171",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multicoin-address-validator": "^0.5.0",
    "@types/node": "^14.17.5",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/redis": "^2.8.31",
    "@types/sharp": "^0.28.5",
    "@types/supertest": "^2.0.11",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "circular-dependency-plugin": "^5.2.2",
    "cross-env": "^7.0.3",
    "dynamodb-admin": "^4.0.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^2.5.4",
    "jest": "^26.6.3",
    "multicoin-address-validator": "^0.5.10",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
